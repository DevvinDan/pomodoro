<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devvindan.pomodoro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/pomodoro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pomodoro"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PomodoroMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PomodoroMain" />
        <activity android:name=".PomodoroSettings" />
        <activity android:name=".PomodoroInfo"></activity>
    </application>

</manifest>